# Generate the CXF Java classes during the compile task
> compile

# Check if the implementation is generated
$ exists target/scala-2.11/src_managed/cxf/simple/acme/simple/Test.java
$ exists target/scala-2.11/src_managed/main/acme/simple/Test.java
$ exists target/scala-2.11/src_managed/cxf/simple/acme/simple/TestPortTypeImpl.java
$ exists target/scala-2.11/src_managed/main/acme/simple/TestPortTypeImpl.java

# Check if the client is generated
$ exists target/scala-2.11/src_managed/cxf/simple/acme/simple/TestPortType_TestPort_Client.java
$ exists target/scala-2.11/src_managed/main/acme/simple/TestPortType_TestPort_Client.java

# Check if the server is generated
$ exists target/scala-2.11/src_managed/cxf/simple/acme/simple/TestPortType_TestPort_Server.java
$ exists target/scala-2.11/src_managed/main/acme/simple/TestPortType_TestPort_Server.java
